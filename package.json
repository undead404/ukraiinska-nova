{
  "dependencies": {
    "@atproto/api": "0.16.7",
    "axios": "~1.11.0",
    "dotenv": "~17.2.2",
    "mustache": "~4.2.0",
    "node-telegram-bot-api": "0.66.0",
    "spotify-web-api-node": "~5.0.2",
    "valibot": "~1.1.0"
  },
  "description": "Scraper for new music releases using Spotify API",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/mustache": "~4.2.6",
    "@types/node": "22",
    "@types/node-telegram-bot-api": "0.64.10",
    "@types/spotify-web-api-node": "~5.0.2",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-vitest": "0.5.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "main": "dist/index.js",
  "name": "spotify-release-scraper",
  "scripts": {
    "build": "tsc",
    "collect-artist-ids": "node --import tsx/esm src/scripts/collect-artist-ids/index.ts",
    "create-post": "node dist/scripts/create-post/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "post-to-bluesky": "node dist/scripts/post-to-bluesky/index.js",
    "post-to-telegram": "node dist/scripts/post-to-telegram/index.js",
    "retrieve-releases": "node dist/scripts/retrieve-releases/index.js",
    "test": "vitest",
    "watch": "tsc -w"
  },
  "type": "module",
  "version": "1.0.0"
}
